                  }
                ],
                "from": {
                  "key_code": "non_us_backslash",
                  "modifiers": { "mandatory": ["left_command"] }
                },
                "to": [{ "key_code": "launchpad" }],
                "type": "basic"
              },
              {
                "conditions": [
                  {
                    "input_sources": [
                      {
                        "input_source_id": "com.apple.keylayout.US",
                        "language": "en"
                      }
                    ],
                    "type": "input_source_if"
                  }
                ],
                "from": {
                  "key_code": "grave_accent_and_tilde",
                  "modifiers": { "mandatory": ["left_command"] }
                },
                "to": [{ "key_code": "launchpad" }],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Activate Launchpad with cmd + < in German Mode",
            "manipulators": [
              {
                "conditions": [
                  {
                    "input_sources": [
                      {
                        "input_source_id": "com.apple.keylayout.German",
                        "language": "de"
                      }
                    ],
                    "type": "input_source_if"
                  }
                ],
                "from": {
                  "key_code": "grave_accent_and_tilde",
                  "modifiers": { "mandatory": ["left_command"] }
                },
                "to": [{ "key_code": "launchpad" }],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Swap Y and Z keys when in US English",
            "manipulators": [
              {
                "conditions": [
                  {
                    "input_sources": [
                      {
                        "input_source_id": "com.apple.keylayout.US",
                        "language": "en"
                      }
                    ],
                    "type": "input_source_if"
                  }
                ],
                "from": { "key_code": "y" },
                "to": { "key_code": "z" },
                "type": "basic"
              },
              {
                "conditions": [
                  {
                    "input_sources": [
                      {
                        "input_source_id": "com.apple.keylayout.US",
                        "language": "en"
                      }
                    ],
                    "type": "input_source_if"
                  }
                ],
                "from": { "key_code": "z" },
                "to": { "key_code": "y" },
                "type": "basic"
              }
            ]
          },
          {
            "description": "Toggle Do Not Disturb with F6",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "f6"
                },
                "to": [
                  {
                    "shell_command": "osascript -e 'set current_state to do shell script \"defaults -currentHost read ~/Library/Preferences/ByHost/com.apple.notificationcenterui doNotDisturb\"' -e 'if current_state is \"1\" then' -e '    do shell script \"defaults -currentHost write ~/Library/Preferences/ByHost/com.apple.notificationcenterui doNotDisturb -boolean false && killall NotificationCenter\"' -e 'else' -e '    do shell script \"defaults -currentHost write ~/Library/Preferences/ByHost/com.apple.notificationcenterui doNotDisturb -boolean true && killall NotificationCenter\"' -e 'end if'"
                  }
                ]
              }
            ]
          },
          {
            "description": "Cmd+X to cut files in Finder",
            "manipulators": [
              {
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": ["^com.apple.finder"]
                  }
                ],
                "from": {
                  "key_code": "x",
                  "modifiers": {
                    "mandatory": ["command"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [
                  {
                    "shell_command": "osascript -e 'tell application \"Finder\" to set the clipboard to (POSIX path of (the selection as alias))' -e 'tell application \"Finder\" to set itemsToCut to the selection' -e 'tell application \"Finder\" to set the clipboard to (get the clipboard) & \"cut\"'"
                  }
                ]
              }
            ]
          },
          {
            "description": "Cmd+V to paste cut or copied files in Finder",
            "manipulators": [
              {
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": ["^com.apple.finder"]
                  }
                ],
                "from": {
                  "key_code": "v",
                  "modifiers": {
                    "mandatory": ["command"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [
                  {
                    "shell_command": "osascript -e 'set clipboardContents to the clipboard' -e 'if clipboardContents ends with \"cut\" then' -e 'set sourceFolder to (text 1 thru -4 of clipboardContents)' -e 'tell application \"Finder\" to move (POSIX file sourceFolder as alias) to (get insertion location)' -e 'tell application \"Finder\" to delete itemsToCut' -e 'else' -e 'tell application \"Finder\" to paste' -e 'end if'"
                  }
                ]
              }
            ]
          },
          {
            "description": "Open Warp Terminal in current Finder location with Cmd+Option+T",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "t",
                  "modifiers": { "mandatory": ["command", "option"] }
                },
                "to": [
                  {
                    "shell_command": "osascript -e 'tell application \"Finder\" to set dir to (POSIX path of (insertion location as alias))' -e 'tell application \"Warp\" to activate' -e 'tell application \"System Events\" to tell process \"Warp\" to keystroke \"t\" using command down' -e 'delay 0.5' -e 'tell application \"System Events\" to tell process \"Warp\" to keystroke \"cd \" & quoted form of dir & \"\n\"'"
                  }
                ]
              }
            ]
          }
        ]
      },
