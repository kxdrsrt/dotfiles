[user]
	name = kadir.sert
	email = kadir.sert@siemens.com

[core]
    editor = nano
    autocrlf = input

[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    lg = log --oneline --graph --decorate --all

[color]
    ui = auto

[push]
    default = simple

[merge]
    tool = vimdiff

[diff]
    tool = vimdiff

[credential]
    helper = store